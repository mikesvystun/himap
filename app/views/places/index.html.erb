<div id="map"></div>

<script type="text/javascript">

  handler = Gmaps.build('Google');
  handler.buildMap({ provider: {}, internal: {id: 'map'}}, function(){


    handler.getMap().setCenter({lat: 49, lng: 32});
    handler.getMap().setZoom(6);


    var h = <%= raw @hash.to_json %>;
    for (var k in h) { 
      var o = {}; 
      o['lat']=h[k]['lat']; 
      o['lng']=h[k]['lng']; 
      setTimeout(addOne, h[k]['founded']*10, o);
    };
  });






function addOne(m) {
  handler.addMarker(m)
}

</script>
